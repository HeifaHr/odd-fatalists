{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-mask",
          "-moz-backface-visibility",
          "-webkit-box-reflect"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off"
  }
}